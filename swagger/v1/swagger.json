{
  "swagger": "2.0",
  "info": {
    "title": "API V1",
    "version": "v1"
  },
  "paths": {
    "/produces": {
      "post": {
        "summary": "Creates a produce",
        "tags": [
          "produces"
        ],
        "consumes": [
          "application/json",
          "application/xml"
        ],
        "parameters": [
          {
            "name": "produce",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "duration": {
                  "type": "string"
                }
              },
              "required": [
                "name",
                "duration"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "produce created"
          },
          "422": {
            "description": "invalid request"
          }
        }
      }
    },
    "/produces/{id}": {
      "get": {
        "summary": "Retrieves a produce",
        "tags": [
          "produces"
        ],
        "produces": [
          "application/json",
          "application/xml"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "name found",
            "schema": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "name": {
                  "type": "string"
                },
                "duration": {
                  "type": "string"
                }
              },
              "required": [
                "id",
                "name",
                "duration"
              ]
            }
          },
          "404": {
            "description": "produce not found"
          }
        }
      }
    }
  }
}