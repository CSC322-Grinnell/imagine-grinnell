<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @garden.name %>
</p>

<p>
  <strong>Address:</strong>
  <%= @garden.address %>
</p>

<p>
  <strong>Lat:</strong>
  <%= @garden.lat %>
</p>

<p>
  <strong>Long:</strong>
  <%= @garden.long %>
</p>

<p>
  <strong>Contact name:</strong>
  <%= @garden.contact_name %>
</p>

<p>
  <strong>Contact number:</strong>
  <%= @garden.contact_number %>
</p>

<p>
  <strong>Email:</strong>
  <%= @garden.email %>
</p>

<p>
  <strong>Produce:</strong>
    <%= @garden.GardenProduces.each do |prod| %>
      <%= Produce.all.find_by(id: prod.produce_id).name %>
    <% end %>
</p>


